#include <stdio.h>
void printArray(int arr[], int n) {
  int i;
  printf("ELEMENTS IN THE ARRAY ARE \n");
  for (i = 0; i < n; i++) {
    printf("%d ", arr[i]);
    printf("\n");
  }
}
int main() {
  int arr[100], n;
  printf("Enter the size of the array\n");
  scanf("%d", &n);
  printf("Enter the elements of the array\n");
  for (int i = 0; i < n; i++) {
    scanf("%d", &arr[i]);
  }
  printArray(arr, n);
  return 0;
}